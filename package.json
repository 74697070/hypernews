{
  "name": "hypernews",
  "version": "1.0.0",
  "description": "hacker news implementation with hyperapp",
  "scripts": {
    "start": "npm run serve",
    "serve": "pushstate-server public 8080",
    "build": "browserify -t babelify src/main.js -o public/bundle.js",
    "watch": "watchify -p [ browserify-livereload ] -t babelify src/main.js -o public/bundle.js -v",
    "dev": "npm run serve & npm run watch"
  },
  "keywords": [
    "hyperapp",
    "hackernews"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-preset-latest": "^6.22.0",
    "classnames": "^2.2.5",
    "firebase": "^3.6.10",
    "hyperapp": "^0.7.1",
    "pushstate-server": "^2.2.1"
  },
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "browserify-livereload": "^1.0.9",
    "http-server": "^0.9.0",
    "watchify": "^3.9.0"
  }
}
